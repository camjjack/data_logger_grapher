{
  "name"    : "data_logger_grapher",
  "productName"    : "data_logger_grapher",
  "version" : "0.0.1",
  "main"    : "main.js",
  "browserify": {
      "transform": [
        "browserify-shim"
      ]
    },
    "browser": {
      "materialize": "./node_modules/materialize-css/dist/js/materialize.min.js"
    },
    "browserify-shim": {
      "materialize": {
          "exports" : "materialize"
          }
    },
    "scripts": {
        "start": "electron .",
        "package": "electron-packager ./ data_logger_grapher --all --out ../app --version 0.0.1 --overwrite"
    },
    "devDependencies": {
        "electron-packager": "^5.0.1",
        "electron-prebuilt": "^0.30.3"
    },
    "dependencies": {
        "fast-csv": "^1.1.0",
        "hammerjs": "^2.0.6",
        "materialize-css": "^0.97.6"
    }
}
